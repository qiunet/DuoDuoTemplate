<?xml version="1.0" encoding="UTF-8"?>
<cache_entity key="playerId"
			  name="PlayerDo"
			  tableName="player"
			  packageName="com.game.server.basic.player"
			  xmlns="https://github.com/qiunet/DuoDuo/CacheEntity"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="https://github.com/qiunet/DuoDuo/CacheEntity
			   https://raw.githubusercontent.com/qiunet/DuoDuo/master/ProjectInit/src/main/resources/xsd/CacheEntity.xsd"
>
	<field name="playerId" type="long" comment="玩家id" />
	<field name="openId" type="String" comment="玩家openId"/>
	<field name="name" type="String" comment="玩家名称"/>
	<field name="exp" type="long" comment="玩家经验" />
	<field name="level" type="int" comment="玩家等级" />
	<field name="m1" type="long" comment="玩家游戏货币" />
	<field name="m2" type="long" comment="玩家人民币代币" />

	<constructor>
		<constructor_arg field="playerId"/>
	</constructor>
</cache_entity>
